<?xml version="1.0" encoding="UTF-8"?>
<persistence xmlns="https://xmlns.jcp.org/xml/ns/persistence"
             xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
             xsi:schemaLocation="https://jakarta.ee/xml/ns/persistence https://jakarta.ee/xml/ns/persistence/persistence_3_0.xsd"
             version="3.0">
  <persistence-unit name="main">
    <class>com.hb.cda.model.Dev</class>
    <class>com.hb.cda.model.Project</class>
    <class>com.hb.cda.model.PorteurDeProjet</class>
    <class>com.hb.cda.model.User</class>
    <class>com.hb.cda.model.Candidature</class>
    <class>com.hb.cda.model.enums.StatusCandidature</class>
    <class>com.hb.cda.model.enums.StatusProject</class>
    <class>com.hb.cda.model.enums.Technologies</class>
    <class>com.hb.cda.model.enums.Thematique</class>
    <properties>
      <property name="jakarta.persistence.jdbc.url" value="jdbc:h2:mem:test"/>
              <property name="hibernate.hbm2ddl.auto" value="create"/>
              <property name="hibernate.show_sql" value="true"/>
          </properties>
  </persistence-unit>
</persistence>
